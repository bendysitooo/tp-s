#include <iostream>
#include <vector>
#include <string>
using namespace std;
class Alumno {
public:
    string nombre;
    float nota1, nota2, nota3;

    void ingresarDatos() {
        cout << "Nombre: "<< endl;
        cin >> nombre;
        cout << "Nota 1: "<< endl;
        cin >> nota1;
        cout << "Nota 2: "<< endl;
        cin >> nota2;
        cout << "Nota 3: "<< endl;
        cin >> nota3;
    }

    void mostrarDatos() const {
        cout << "Nombre: " << nombre<< endl << ", Nota 1: " << nota1<< endl << ", Nota 2: " << nota2<< endl << ", Nota 3: " << nota3 << endl;
    }
};

int main() {
    vector <Alumno> alumnos;
    int numAlumnos;

    cout << "Ingrese el nÃºmero de alumnos: " << endl;
    cin >> numAlumnos;

    for (int i = 0; i < numAlumnos; ++i) {
        Alumno a;
        a.ingresarDatos();
        alumnos.push_back(a);
    }

    cout << "Datos de los alumnos:" << endl;
    for (const auto& a : alumnos) {
        a.mostrarDatos();
    }

    return 0;
}
