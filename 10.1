#include <iostream>
#include <vector>
#include <string>
using namespace std;
class Alumno {
public:
    std::string nombre;
    float nota1, nota2, nota3;

    void ingresarDatos() {
        cout << "Nombre: ";
        cin >> nombre;
        cout << "Nota 1: ";
        cin >> nota1;
        cout << "Nota 2: ";
        cin >> nota2;
        cout << "Nota 3: ";
        cin >> nota3;
    }

    void mostrarDatos() const {
        cout << "Nombre: " << nombre << ", Nota 1: " << nota1 << ", Nota 2: " << nota2 << ", Nota 3: " << nota3 << endl;
    }
};

int main() {
    vector<Alumno> alumnos;
    int numAlumnos;

    cout << "Ingrese el nÃºmero de alumnos: ";
    cin >> numAlumnos;

    for (int i = 0; i < numAlumnos; ++i) {
        Alumno a;
        a.ingresarDatos();
        alumnos.push_back(a);
    }

    cout << "\n Datos de los alumnos:\n";
    for (const auto& a : alumnos) {
        a.mostrarDatos();
    }

    return 0;
}
